    "tokenColors": [{
            "settings": {
                "foreground": "#F1ECDE",
                "background": "#122125"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#5B7273"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#98BC37"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "variable.function",
                "support.function",
                "meta.function-call.generic"
            ],
            "settings": {
                "foreground": "#95B8E6"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage",
                "entity.name.tag",
                "support.function.magic.python",
                "keyword.other.use",
                "keyword.other.function.use",
                "keyword.other.namespace",
                "keyword.other.new",
                "keyword.other.special-method",
                "keyword.other.use-as"
            ],
            "settings": {
                "foreground": "#B3AAEE"
            }
        },
        {
            "name": "Type",
            "scope": [
                "entity.name.class",
                "support.type",
                "support.class",
                "entity.other.inherited-class",
                "entity.other.attribute-name",
                "markup.inserted.git_gutter",
                "support.type.sys-types",
                "keyword.type",
                "storage.type",
                "storage.type.primitive",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#6E5CE0"
            }
        },
        {
            "name": "Normal",
            "scope": [
                "variable",
                "variable.parameter",
                "entity.name.variable",
                "support.other.variable",
                "support.constant",
                "keyword.operator",
                "entity.name.function.operator",
                "storage.modifier.pointer",
                "storage.modifier.reference",
                "storage.type.cs"
            ],
            "settings": {
                "foreground": "#F1ECDE"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#FA5F8F"
            }
        },
        {
            "name": "Special",
            "scope": [
                "constant.character.escape",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FED06E"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ED4566"
            }
        },
        {
            "name": "Inserted",
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#B3AAEE"
            }
        },
        {
            "name": "Deleted",
            "scope": [
                "markup.deleted",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#FF5377"
            }
        },
        {
            "name": "Changed",
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#87d7d7"
            }
        },
        {
            "name": "URL",
            "scope": [
                "*url*",
                "*link*",
                "*uri*"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#F1ECDE"
            }
        },
        {
            "name": "Markdown - Markup Raw Inline Punctuation",
            "scope": [
                "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
            ],
            "settings": {
                "foreground": "#65737E"
            }
        },
        {
            "name": "Markdown - Heading",
            "scope": [
                "markdown.heading",
                "markup.heading | markup.heading entity.name",
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#B3AAEE"
            }
        },
        {
            "name": "Markup - Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e0ebe9"
            }
        },
        {
            "name": "Markup - Bold",
            "scope": [
                "markup.bold",
                "markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#e0ebe9"
            }
        },
        {
            "name": "Markup - Bold-Italic",
            "scope": [
                "markup.bold markup.italic",
                "markup.italic markup.bold",
                "markup.quote markup.bold",
                "markup.bold markup.italic string",
                "markup.italic markup.bold string",
                "markup.quote markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#e0ebe9"
            }
        },
        {
            "name": "Markup - Underline",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#e0ebe9"
            }
        },
        {
            "name": "Markup - Strike",
            "scope": [
                "markup.strike"
            ],
            "settings": {
                "fontStyle": "strike",
                "foreground": ""
            }
        },
        {
            "name": "Markup - Section",
            "scope": [
                "entity.name.section",
                "meta.separator.markdown",
                "markup.heading.setext"
            ],
            "settings": {
                "foreground": "#fffbec"
            }
        },
        {
            "name": "Markdown - Blockquote",
            "scope": [
                "markup.quote punctuation.definition.blockquote.markdown"
            ],
            "settings": {
                "background": "#65737E",
                "foreground": "#65737E"
            }
        },
        {
            "name": "Markup - Quote",
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#65737a"
            }
        },
        {
            "name": "Markdown - Link",
            "scope": [
                "string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#87d7d7"
            }
        },
        {
            "name": "Markdown - Link Description",
            "scope": [
                "string.other.link.description.title.markdown"
            ],
            "settings": {
                "foreground": "#e0ebe9"
            }
        },
        {
            "name": "Markdown - Link Anchor",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#87d7d7"
            }
        },
        {
            "name": "Markdown - Raw Block Fenced",
            "scope": [
                "markup.raw.block.fenced.markdown"
            ],
            "settings": {
                "foreground": "#00000050"
            }
        },
        {
            "name": "Markdown - Fenced Block",
            "scope": [
                "punctuation.definition.fenced.markdown"
            ],
            "settings": {
                "foreground": "#00000050"
            }
        },
        {
            "name": "Markdown - Fenced Block, List",
            "scope": [
                "markup.raw.block.fenced.markdown",
                "variable.language.fenced.markdown",
                "punctuation.section.class.end"
            ],
            "settings": {
                "foreground": "#e0ebe9"
            }
        },
        {
            "name": "Markup - Table",
            "scope": [
                "markup.table"
            ],
            "settings": {
                "foreground": "#e0ebe9"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#0E4FFF"
            }
        }
    ]
}
